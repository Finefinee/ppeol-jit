# Part 4 - Assert in functions
print("=== Part 4 - Assert in functions ===")

fn validate_positive(num) {
    assert num > 0, "Number must be positive"
    return num
}

print("Testing validate_positive(5)...")
try {
    validate_positive(5)
    print("validate_positive(5): OK")
    print("Testing validate_positive(-3)...")
    validate_positive(-3)
} catch AssertionError as e {
    print("Validation failed:", e)
}
print()

print("=== Part 4 completed! ===")
