# 여러 가지 예외 타입 테스트

fn test_index_error() {
    let arr = [1, 2, 3]
    let x = arr[10]  # IndexError
}

fn test_key_error() {
    let dict = {"a": 1, "b": 2}
    let x = dict["c"]  # KeyError
}

fn test_assert_error() {
    let x = 5
    assert x == 10, "x should be 10"  # AssertionError
}

fn test_throw_error() {
    throw "Custom error message"  # RuntimeError
}

# 각 테스트 실행
print("Testing IndexError:")
test_index_error()
