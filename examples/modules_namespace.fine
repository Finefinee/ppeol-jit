# 모듈 네임스페이스 테스트 (v2.0 기능)

import math

print("=== 모듈 네임스페이스 테스트 ===")

# math 모듈의 abs 함수 호출
let result1 = math.abs(-5)
print("math.abs(-5) =", result1)

# math 모듈의 max 함수 호출
let result2 = math.max(10, 20)
print("math.max(10, 20) =", result2)

# math 모듈의 상수 접근
let pi_value = math.PI
print("math.PI =", pi_value)

# 계산 테스트
let area = math.pow(5, 2) * math.PI
print("반지름 5인 원의 넓이 =", area)

print("테스트 완료!")
